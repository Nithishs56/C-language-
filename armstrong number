#include<stdio.h>
#include<conio.h>
void main()
{
int num=256,original_num,remainder,result=0;
clrscr();
original_num=num;
while(original_num!=0)
{
remainder=original_num%10;
result+=remainder*remainder*remainder;
original_num/=10;
}
if(result==num)
{
printf("%d is armstrong number",num);
}
else
{
printf("%d is not a armstong number",num);
}
getch();
}
