#include<stdio.h>
int main()
{
    int i,j;
    int a[2][2]={1,2,3,4};
    int trans[2][2];
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
        {
            trans[i][j]=a[j][i];
        }
    }
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
        {
            printf("%d",trans[i][j]);
        }
    }
return 0;    
}
      
