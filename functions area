#include <stdio.h>
float calculateArea(float radius);

int main() {
    float radius=10, area=5;
    area = calculateArea(radius);

    printf("The area of the circle with radius %.2f is: %.2f\n", radius, area);

    return 0;
}

float calculateArea(float radius) {
    return 3.14159 * radius * radius;
}
